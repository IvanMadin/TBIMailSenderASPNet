@model IEnumerable<EmailViewModel>

@{
    ViewData["Title"] = "All Emails";
}


<link rel="stylesheet" href="~/css/page.css" />
<link rel="stylesheet" href="~/css/theme.css" />

<partial name="_EmailTablePartial" />

@section Scripts{

    <script src="~/js/aspnet/signalr/dist/browser/signalr.js"></script>
    <script src="~/js/testSignalR.js"></script>

    <script type="text/javascript" charset="utf8" src="~/js/jquerydatatable.js"></script>
    <script type="text/javascript" charset="utf8" src="~/js/datatable.js"></script>

    <script>

        $(document).ready(function () {
            $('#application-form-div').DataTable({
                "order": [[1, "desc"]]
            });
        });
    </script>
}